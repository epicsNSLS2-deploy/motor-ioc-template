{$(PREFIX),  7,  "m$(N)",  "asynMotor",  $(PORT),  2,     "motor $(N)",  degrees,  Pos,  1,     .1,    .2,    0,     1,     .2,    0.01,  5,     100,   -100,  ""}
{$(PREFIX),  8,  "m$(N)",  "asynMotor",  $(PORT),  3,     "motor $(N)",  degrees,  Pos,  1,     .1,    .2,    0,     1,     .2,    0.01,  5,     100,   -100,  ""}
